{
    "configuration": {
        "device": {
            "orientation": "landscape",
            "deviceName": "Desktop"
        }
    },
    "type": "clickpath",
    "version": "1.0",
    "events": [
        {
            "description": "Loading of \"http://codeminter.com/login\"",
            "wait": {
                "waitFor": "page_complete"
            },
            "target": {},
            "type": "navigate",
            "url": "http://codeminter.com/login"
        },
        {
            "description": "click",
            "wait": {
                "waitFor": "page_complete"
            },
            "target": {
                "locators": [
                    {
                        "type": "css",
                        "value": "#username"
                    }
                ]
            },
            "type": "click",
            "button": 0
        },
        {
            "description": "type username",
            "wait": {
                "waitFor": "page_complete"
            },
            "target": {
                "locators": [
                    {
                        "type": "css",
                        "value": "#username"
                    }
                ]
            },
            "type": "keystrokes",
            "textValue": "username",
            "simulateBlurEvent": true,
            "masked": false
        },
        {
            "description": "click",
            "wait": {
                "waitFor": "page_complete"
            },
            "target": {
                "locators": [
                    {
                        "type": "css",
                        "value": "#password"
                    }
                ]
            },
            "type": "click",
            "button": 0
        },
        {
            "description": "type password",
            "wait": {
                "waitFor": "page_complete"
            },
            "target": {
                "locators": [
                    {
                        "type": "css",
                        "value": "#password"
                    }
                ]
            },
            "type": "keystrokes",
            "textValue": "password",
            "simulateBlurEvent": true,
            "masked": false,
            "simulateReturnKey": true
        }
    ]
}
